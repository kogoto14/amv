  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Attach Quarkus",
      "request": "attach",
      "hostName": "localhost",
      "port": 5005,
      "projectName": "amv-backend"
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/amv-frontend"
    }
  ]
}
